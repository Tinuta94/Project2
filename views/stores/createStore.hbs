<form method="POST" action="/stores">
    <label for="name">Name</label>
    <input type="text" name="name"/>

    <label>Address</label>
    <input type="text" name="Address"/>

    <label>Currently Open</label>
    <input type="checkbox"  name="currentlyOpen" value="true"/>

    <label>Phone Number</label>
    <input type="number" name="phoneNumber" id="pr"/>

    <input type="submit" value="Create Store" id="cr"/>
  
</form>